server {
	port 4243
	server_name daddy_christmas;
	error_page 404 404.html;
	error_page 501 501.html;
	root ./srcs/server;
	body_size 10000000;
	method GET POST DELETE;

	location /hello
	{
		root ./srcs/server;
		index hello.html;
		method	GET POST DELETE;
	}

	location / {
		root ./srcs/server;
		index home.html;
		method	GET POST DELETE;
	}
	
	location /home {
		root ./srcs/server;
		index home.html;
		method	GET POST DELETE;
	}
	
	location /upload {
		root ./srcs/server/upload;
		index upload.html;
		method	GET POST DELETE;
	}

	location /delete {
		root ./srcs/server/delete;
		index delete.html;
		method	GET POST DELETE;
	}
}
