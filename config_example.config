server {
	port 4243
	server_name daddy_christmas;
	error_page 404 404.html;
	error_page 405 404.html;
	error_page 413 413.html;
	error_page 501 501.html;
	root ./srcs/server;
	body_size 10;
	method GET POST DELETE;

	location /hello
	{
		root ./srcs/server;
		index hello.html;
		method GET;
	}

	location /javascript
	{
		root ./srcs/server;
		index javascript.html;
		method GET;
	}

	location /image
	{
		root ./srcs/server;
		index image.html;
		method ;
	}

	location /video
	{
		root ./srcs/server;
		index video.html;
		method GET;
	}

	location / {
		root ./srcs/server;
		index home.html;
		method	GET POST DELETE;
	}
	
	location /home {
		root ./srcs/server;
		index home.html;
		method	GET POST DELETE;
	}
	
	location /upload {
		root ./srcs/server/upload;
		index upload.html;
		method	GET POST DELETE;
	}

	location /delete {
		root ./srcs/server/delete;
		index delete.html;
		method	GET POST DELETE;
