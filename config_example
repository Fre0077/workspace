server {
	port 4243;
	server_name localhost;
	error_page 404 404.html;
	error_page 501 501.html;
	index home.html;
	root ./srcs/server;


	location /hello {
		root ./srcs/server;
		index hello.html;
	}

	location / {
		root ./srcs/server;
		index home.html;
	}
	
	location /home {
		root ./srcs/server;
		index home.html;
	}
	
	location /upload {
		root ./srcs/server/upload;
		index home.html;
	}
}